{
    "name": "@hiveai/adapters-telegram",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": ["dist"],
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts --no-config",
        "dev": "tsup src/index.ts --format esm --dts --watch"
    },
    "dependencies": {
        "@hiveai/utils": "workspace:*",
        "@hiveai/messaging": "workspace:*",
        "node-telegram-bot-api": "^0.64.0",
        "telegraf": "^4.15.3",
        "events": "^3.3.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/events": "^3.0.3",
        "tsup": "8.3.5",
        "typescript": "^5.0.0"
    }
} 